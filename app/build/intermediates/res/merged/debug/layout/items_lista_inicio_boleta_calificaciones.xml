<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="horizontal"
    android:background="@color/colorFondoContenido">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Materia:"
        android:id="@+id/textView26"
        android:layout_below="@+id/textView24"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="0"
        android:layout_column="0"
        android:textColor="@color/colorTextoContenido" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="materia"
        android:id="@+id/item_boleta_calificaciones_txtmateria"
        android:layout_row="0"
        android:layout_column="1"
        android:layout_columnSpan="3"
        android:textColor="@color/colorTextoContenido"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView27"
        android:layout_toEndOf="@+id/textView27" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Ordinario:"
        android:id="@+id/textView27"
        android:layout_below="@+id/textView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="1"
        android:layout_column="0"
        android:textColor="@color/colorTextoContenido" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="ordinario"
        android:id="@+id/item_boleta_calificaciones_txtordinario"
        android:layout_row="1"
        android:layout_column="1"
        android:textColor="@color/colorTextoContenido"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_alignTop="@+id/textView27"
        android:layout_toRightOf="@+id/textView27"
        android:layout_toEndOf="@+id/textView27" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Extraordinario:"
        android:layout_row="2"
        android:layout_column="0"
        android:textColor="@color/colorTextoContenido"
        android:id="@+id/textView9"
        android:layout_below="@+id/textView27"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="extraordinario"
        android:id="@+id/item_boleta_calificaciones_txtextraordinario"
        android:layout_row="2"
        android:layout_column="1"
        android:textColor="@color/colorTextoContenido"
        android:layout_below="@+id/item_boleta_calificaciones_txtordinario"
        android:layout_toRightOf="@+id/item_boleta_calificaciones_txtmateria"
        android:layout_toEndOf="@+id/item_boleta_calificaciones_txtmateria" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Kardex:"
        android:layout_row="1"
        android:layout_column="2"
        android:textColor="@color/colorTextoContenido"
        android:id="@+id/textView10"
        android:layout_alignTop="@+id/item_boleta_calificaciones_txtordinario"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="kardex"
        android:id="@+id/item_boleta_calificaciones_txtkardexo"
        android:layout_row="1"
        android:layout_column="3"
        android:textColor="@color/colorTextoContenido"
        android:layout_alignTop="@+id/textView10"
        android:layout_toRightOf="@+id/textView11"
        android:layout_toEndOf="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Kardex:"
        android:layout_row="2"
        android:layout_column="2"
        android:textColor="@color/colorTextoContenido"
        android:id="@+id/textView11"
        android:layout_alignTop="@+id/item_boleta_calificaciones_txtextraordinario"
        android:layout_toLeftOf="@+id/item_boleta_calificaciones_txtkardexe"
        android:layout_toStartOf="@+id/item_boleta_calificaciones_txtkardexe" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="kardex"
        android:id="@+id/item_boleta_calificaciones_txtkardexe"
        android:layout_row="2"
        android:layout_column="3"
        android:textColor="@color/colorTextoContenido"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="10dp"
        android:background="@android:color/black"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
